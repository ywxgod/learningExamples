        "babel-preset-react": "^6.24.1",
        "css-loader": "^0.28.11",
        "file-loader": "^1.1.11",
        "html-webpack-plugin": "^3.2.0",
        "mini-css-extract-plugin": "^0.4.0",
        "node-sass": "^4.9.0",
        "nodemon": "^1.17.4",
        "optimize-css-assets-webpack-plugin": "^4.0.1",
        "postcss-loader": "^2.1.5",
        "sass-loader": "^7.0.1",
        "shx": "^0.2.2",
        "style-loader": "^0.21.0",
